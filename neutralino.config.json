{
  "applicationId": "fluentcode",
  "version": "0.2.7",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "window.*",
    "clipboard.*",
    "filesystem.*",
    "os.*",
    "debug.*",
    "events.*",
    "computer.*",
    "storage.*",
    "extensions.*"
  ],
  "globalVariables": {
    "LOADED": [],
    "LOADING_TIME": null
  },
  "modes": {
    "window": {
      "title": "Fluent Code",
      "width": 800,
      "height": 600,
      "minWidth": 700,
      "minHeight": 500,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": true,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "fluentcode",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}